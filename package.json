{
  "name": "stgo-ci-helper",
  "displayName": "stgo-ci-helper",
  "repository": "https://github.com/nishants/stgo-ci-helper",
  "publisher": "saxo-india",
  "description": "stgo ci helper vscode extension",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.stgoci"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.stgoci",
        "title": "stgoci"
      }
    ]
  },
  "scripts": {
    "publish": "vsce publish -p *",
    "build": "run-p build:ui build:extension",
    "build:extension": "vsce package",
    "build:ui": "webpack --config webpack.config.js",
    "vscode:prepublish": "run-s build:ui compile",
    "compile": "webpack --mode development --config webpack/webpack.extension.config.js",
    "compile:e2e": "webpack --mode development --config webpack/webpack.e2e.config.js",
    "lint:vs": "eslint src --ext ts",
    "lint:ui": "eslint ui",
    "lint": "run-p lint:ui lint:vs",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "run-p test:extension test:lib",
    "test:extension": "node ./out/test/runTest.js",
    "test:lib": "yarn jest ./src/lib",
    "e2e-test": "npm run compile && npm run compile:e2e && extest setup-and-run out/e2e/*.js",
    "webpack": "webpack --mode development --config webpack/webpack.extension.config.js",
    "webpack-dev": "webpack --mode development --watch --config webpack/webpack.extension.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/chai": "^4.2.11",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mocha": "^7.1.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^5.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.7.5",
    "vsce": "^1.74.0",
    "vscode-extension-tester": "^2.5.2",
    "vscode-test": "^1.3.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "azure-devops-node-api": "^10.1.0",
    "cherow": "^1.6.9",
    "estraverse": "^5.0.0",
    "execa": "^4.0.0",
    "os": "^0.1.1"
  }
}
